{
  "projectType": "library",
  "root": "libs/paris",
  "sourceRoot": "libs/paris/src",
  "prefix": "ng13",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/paris"],
      "options": {
        "jestConfig": "libs/paris/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/paris/src/**/*.ts",
          "libs/paris/src/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
